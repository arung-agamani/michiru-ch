{
    "tasks": {
        "dev": "deno run -A --node-modules-dir npm:vite",
        "build": "deno run -A --node-modules-dir npm:vite build",
        "preview": "deno run -A --node-modules-dir npm:vite preview",
        "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/"
    },
    "compilerOptions": {
        "lib": ["ES2020", "DOM", "DOM.Iterable"],
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "jsxImportSourceTypes": "@types/react"
    },
    "imports": {
        "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
        "@headlessui/react": "npm:@headlessui/react@^2.2.0",
        "@radix-ui/themes": "npm:@radix-ui/themes@^3.2.1",
        "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.0.12",
        "@tanstack/react-query": "npm:@tanstack/react-query@^5.67.3",
        "@tanstack/react-query-devtools": "npm:@tanstack/react-query-devtools@^5.67.3",
        "@tanstack/react-table": "npm:@tanstack/react-table@^8.21.2",
        "@types/react": "npm:@types/react@^19.0.10",
        "@types/react-dom": "npm:@types/react-dom@^19.0.4",
        "@vitejs/plugin-react-swc": "npm:@vitejs/plugin-react-swc@^3.8.0",
        "clsx": "npm:clsx@^2.1.1",
        "jotai": "npm:jotai@^2.12.2",
        "ky": "npm:ky@^1.7.5",
        "react": "npm:react@^19.0.0",
        "react-dom": "npm:react-dom@^19.0.0",
        "react-hook-form": "npm:react-hook-form@^7.54.2",
        "react-router-dom": "npm:react-router-dom@^7.3.0",
        "tailwind-merge": "npm:tailwind-merge@^3.0.2",
        "tailwindcss": "npm:tailwindcss@^4.0.12",
        "vite": "npm:vite@^6.1.1"
    },
    "nodeModulesDir": "auto"
}
